<h2 class="card-title">Country: {{name}}</h2>
<div class="card-country">
    <div>
        <p class="card-capital">Capital: {{capital}}</p>
        <p class="card-population">Population: {{population}}</p>
        <p class="card-population">Languages: </p>
        <ul class="card-languages">
            {{#each this.languages}}
            <li class="list-language-item">{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="card-flag">
        <img src="{{flag}}" alt="{{name}}">
    </div>
</div>